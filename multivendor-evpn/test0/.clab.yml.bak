name: test0

mgmt:
  network: netlab_mgmt
  ipv4-subnet: 192.168.121.0/24
  # Note: 'start' not validated
topology:
  nodes:
    r1:
      mgmt-ipv4: 192.168.121.101
      kind: vr-ftosv
      image: vrnetlab/vr-ftosv:10.5.6.0
      runtime: docker
      group: as100
    r2:
      mgmt-ipv4: 192.168.121.102
      kind: vr-aoscx
      image: vrnetlab/vr-aoscx:20231110145644
      runtime: docker
      group: as100

    test0_2:
      kind: bridge
    test0_3:
      kind: bridge

  links:
  - endpoints:
    - "r1:eth1"
    - "r2:eth1"
  - endpoints:
    - "r1:eth2"
    - "test0_2:r1_eth2"
  - endpoints:
    - "r2:eth2"
    - "test0_3:r2_eth2"
